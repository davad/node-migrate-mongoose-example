{
  "name": "migrate-mongoose-example",
  "version": "1.0.0",
  "description": "Example migration implementation for Mongoose",
  "main": "index.js",
  "dependencies": {
    "confidence": "^4.0.1",
    "dotenv": "^6.2.0",
    "loggin": "^3.0.1",
    "migrate": "^1.6.2",
    "mongoose": "^5.4.14"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npm run migrate:list",
    "migrate:up": "migrate up --store='./config/migrations/store.js'",
    "migrate:down": "migrate down --store='./config/migrations/store.js'",
    "migrate:create": "migrate create --template-file='./config/migrations/template.js'",
    "migrate:list": "migrate list --store='./config/migrations/store.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davad/node-migrate-mongoose-example.git"
  },
  "keywords": [
    "migrate",
    "mongoose",
    "migration",
    "mongo",
    "mongodb"
  ],
  "author": "David Landry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davad/node-migrate-mongoose-example/issues"
  },
  "homepage": "https://github.com/davad/node-migrate-mongoose-example#readme"
}
